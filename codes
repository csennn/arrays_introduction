#include <iostream>
using namespace std;


int main() {
    
    int dizinin_boyutu;
    cin >> dizinin_boyutu;
    
    int * dizinin_adresi = new int[dizinin_boyutu]; 
    
    for (int i = 0; i < dizinin_boyutu; i++){
        cin >> dizinin_adresi[i];
    }
    
    for (int i = (dizinin_boyutu-1); i >=0; i--){
        cout << dizinin_adresi[i] << " " ;
    }
      
    delete [] dizinin_adresi;
      
    return 0;
}
